{
  "name": "pisces-gh-pages",
  "version": "0.0.15",
  "description": "pisces website",
  "scripts": {
    "build": "tarima -fdV",
    "watch": "tarima -wsdVP",
    "start": "npm run watch",
    "svg": "svg-sprite -s --symbol-dest=svg --symbol-prefix=.svg- --ss=sprite.svg --si --sx --shape-id-generator=svg-%s --dest=build assets/svg/*.svg"
  },
  "author": "Noel Delgado <auhcsei@gmail.com> (http://pixelia.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noeldelgado/pisces/issues"
  },
  "homepage": "https://github.com/noeldelgado/pisces#readme",
  "tarima": {
    "bundle": [
      "**/_entries/**"
    ],
    "rollupFile": "rollup.config.js",
    "public": "./",
    "src": "src/**/*",
    "dest": "./build",
    "watch": "**.html",
    "filter": [
      "**/_entries/**"
    ],
    "devPlugins": [
      "tarima-lr"
    ],
    "rename": "**/_entries/**:{filepath/1/_entries}/{filename}.{extname}",
    "bundleOptions": {
      "extensions": {
        "js": "es6",
        "css": "post"
      },
      "postcss": {
        "plugins": [
          "postcss-import",
          "postcss-cssnext",
          "postcss-discard-comments"
        ]
      }
    }
  },
  "dependencies": {
    "basscss": "^8.0.1",
    "gemini-scrollbar": "^1.3.2",
    "normalize.css": "^4.1.1",
    "pisces": "0.0.14",
    "sanitize.css": "^3.3.0",
    "share-url": "^1.0.0",
    "text-gradient": "^0.2.0",
    "tween.js": "^16.3.4"
  },
  "_____devDep": {
    "_": "using old versions installed globally",
    "_tarima": "1.7.0",
    "_tarima-cli": "0.3.7",
    "_tarima-lr": "0.0.8"
  },
  "devDependencies": {
    "buble": "^0.9.3",
    "postcss": "^5.1.0",
    "postcss-cssnext": "^2.5.2",
    "postcss-discard-comments": "^2.0.4",
    "postcss-import": "^8.1.2",
    "rollup": "^0.41.4",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-json": "^2.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "svg-sprite": "^1.3.3"
  }
}
